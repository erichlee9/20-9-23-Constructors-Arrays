package eg2;

public class Employee {
	
	private int id;
	private String name;
	private double salary;
	
	public Employee() {
		// TODO Auto-generated constructor stub
	}

	public Employee(int id, String name, double salary) {
		this.id = id;
		this.name = name;
		this.salary = salary;
	}
	
	@Override //annotation
	public String toString() {
		return "hello "+name+" "+salary;
	}

}
